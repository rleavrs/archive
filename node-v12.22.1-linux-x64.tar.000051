  constructor (props) {
    super(props)
    this.state = {
      isOpen: true
    }
    this.onHide = this.onHide.bind(this)
  }

  onHide () {
    this.setState({isOpen: !this.state.isOpen})
  }

  render () {
    return (
      <div>
        <SectionButton isOpen={this.state.isOpen} onClick={this.onHide}>{this.props.section}</SectionButton>
        {this.state.isOpen &&
          <div>
            {this.props.children}
          </div>
        }
      </div>
    )
  }
}

export default Accordion
