    out = 'Top hits for ' + (args.map(JSON.stringify).join(' ')) + '\n' +
          (new Array(cols)).join('‚Äî') + '\n' +
          out + '\n' +
          (new Array(cols)).join('‚Äî') + '\n' +
          '(run with -l or --long to see more context)'
  }

  output(out.trim())
  cb(null, results)
}
