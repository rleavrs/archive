    const reqOpts = opts.concat({
      method: 'PUT',
      body: JSON.stringify(version),
      headers: {
        'content-type': 'application/json'
      },
      spec
    })
    return otplease(reqOpts, reqOpts => regFetch(url, reqOpts)).then(() => {
      output(`+${t}: ${spec.name}@${version}`)
    })
  })
