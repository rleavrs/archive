    }
    const version = tags[tag]
    delete tags[tag]
    const url = `/-/package/${spec.escapedName}/dist-tags/${encodeURIComponent(tag)}`
    const reqOpts = opts.concat({
      method: 'DELETE',
      spec
    })
    return otplease(reqOpts, reqOpts => regFetch(url, reqOpts)).then(() => {
      output(`-${tag}: ${spec.name}@${version}`)
    })
  })
